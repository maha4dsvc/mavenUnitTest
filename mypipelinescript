pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
                echo 'this is maha'
            }
        }
        stage('clone MAHA LOGIN java project') {
            steps {
                git 'https://github.com/maha4dsvc/mahalogin.git'
            }
        }
        stage('execute maven Targets') {
            steps {
               sh 'mvn clean install'
            }
        }
    }
}
